rule move
    pool = console
    command = mv ./demo ../../../Base/root/ && ninja -t clean

rule cpp
    command = i686-elf-g++ -T link.ld -o demo $in ../../../Libraries/libc.a -fno-builtin -fno-use-cxa-atexit -fno-rtti -fno-exceptions -fno-leading-underscore -nostdlib -ffreestanding -O2 -I../../../Libraries -lgcc

build ./demo demo.o: cpp ./demo.cpp
build move: move
default ./demo
